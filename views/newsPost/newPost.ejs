<h1>New News Blog Post</h1>

<section id="newsBlogPostForm">
  <form action="/newsPost/new" method="post">
    <div><input type="text" name="title" placeholder="Title" size="70"></div>
    <div><textarea name="content" rows="30" cols="50" placeholder="Content"></textarea></div>
    <div><input type="checkbox" name="published" value="true">Publish now?</div>
    <div><input type="submit"</div>
  </form>
</section>
