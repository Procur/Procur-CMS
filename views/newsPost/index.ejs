<h1>Application Updates/ News Blog</h1><hr>

<% posts.forEach(function(post){ %>
  <% if(post.published == true){ %>
    <div><a href="/newsblog/<%= post.id %>"><h3><%= post.title %></h3></a></div>
    <div><strong>Posted: <%= post.createdAt %></strong></div>
    <div><p><%= post.content %></p></div>
    <div><img src="<%= post.images %>"></div>
  <% } %>
<% }); %>

<a href="/newsPost/new">Create New Post</a>
