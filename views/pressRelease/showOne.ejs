
<% if(post.published == true){ %>
  <div><h1><%= post.title %></h1></div>
  <div><p><%= post.createdAt %></p></div>
  <div><p><%= post.abstract %></p></div>
  <div><p><%= post.content %></p></div>
<% } %>

<form method="get" action="file.doc">
<button type="submit">Download the Zip!</button>
</form>


<a href="/pressreleases">Home</a>
<a href="/pressRelease/unpublish/<%= post.slug %>">Unpublish</a>
<a href="/pressRelease/edit/<%= post.slug %>">Edit</a>
