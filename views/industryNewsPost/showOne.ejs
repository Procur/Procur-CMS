<% if(post.published == true){ %>
  <div><h1><%= post.title %></h1></div>
  <div><strong>Posted: <%= post.createdAt %></strong></div>
  <div><p><%= post.content %></p></div>
  <div><img src="<%= post.images %>"></div>
<% } %>

<a href="/industrynews">Home</a>
<a href="/industrynewsPost/unpublish/<%= post.id %>">Unpublish</a>
<a href="/industrynewsPost/edit/<%= post.id %>">Edit</a>
